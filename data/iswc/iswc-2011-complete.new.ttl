<http://data.semanticweb.org/conference/iswc/2011/paper/poster-demo/74>
        a                      swrc:InProceedings ;
        rdfs:label             "How to Represent Knowledge Diversity" ;
        swc:relatedToEvent     <http://data.semanticweb.org/conference/iswc/2011/sessions/poster-session> ;
