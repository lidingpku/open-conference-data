        swrc:link_publisher    <http://dx.doi.org/10.1007/978-3-642-25073-6_41> ;
        swrc:url               <http://dx.doi.org/10.1007/978-3-642-25073-6_41> , <http://iswc2011.semanticweb.org/fileadmin/iswc/Papers/Research_Paper/09/70310640.pdf> ;
