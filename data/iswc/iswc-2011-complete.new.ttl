        swrc:link_publisher    <http://dx.doi.org/10.1007/978-3-642-25093-4_23> ;
        swrc:url               <http://dx.doi.org/10.1007/978-3-642-25093-4_23> , <http://iswc2011.semanticweb.org/fileadmin/iswc/Papers/DC_Proposals/70320313.pdf> ;
