        swrc:link_open_access  <http://iswc2011.semanticweb.org/fileadmin/iswc/Papers/DC_Proposals/70320361.pdf> ;
        swrc:link_publisher    <http://dx.doi.org/10.1007/978-3-642-25093-4_29> ;
        swrc:url               <http://dx.doi.org/10.1007/978-3-642-25093-4_29> , <http://iswc2011.semanticweb.org/fileadmin/iswc/Papers/DC_Proposals/70320361.pdf> ;
